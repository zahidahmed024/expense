{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "paths": {
      "*": [
        "./src/*"
      ],
      "@assets": [
        "./src/assets/"
      ],
      "@constants": [
        "./src/constants/"
      ],
      "@navigations": [
        "./src/navigations/"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@components": [
        "./src/components/"
      ],
      "@services": [
        "./src/services/"
      ],
      "@utils": [
        "./src/utils/"
      ],
      "@store/*": [
        "./src/store/*"
      ],
    }
  }
}